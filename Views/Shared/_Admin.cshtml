
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/Scripts/js")

</head>
<body style="background: url('https://localhost:44327/images/7.jpg')">
    <nav class="navbar navbar-expand-md navbar-light bg-primary">
        @* <div class="container-fluid">

            </div>*@
        <a class="navbar-brand" href="#">INTERCOLLEGE FEST</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="/Home/">Home</a>
                </li>
                <li class="nav-item">
                    <a class="text-light nav-link" href="/Home/Events">Events list</a>

                </li>
                <li class="nav-item">
                    <a class="text-light nav-link" href="/Home">Gallary</a>

                </li>
                <li class="nav-item">
                    <a class="text-light nav-link" href="/Home/Leaderboard">Leaderboard</a>
                </li>
                <li class="nav-item">
                    <a class="text-light nav-link" href="/Login/">Login</a>
                </li>
                <li class="nav-item">
                    <a class="text-light nav-link" href="/Login/Signup">Register</a>
                </li>
            </ul>
        </div>

    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year college fest</p>
        </footer>
    </div>

    @Scripts.Render("~/content/bootstrap")
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj" crossorigin="anonymous"></script>
    @RenderSection("scripts", required: false)
</body>
</html>

